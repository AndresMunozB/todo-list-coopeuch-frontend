{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@microsoft/applicationinsights-react-js": "^3.0.3",
    "@microsoft/applicationinsights-web": "^2.5.8",
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.7.1",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.14.28",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux-logger": "^3.0.8",
    "axios": "^0.21.1",
    "immer": "^8.0.1",
    "localforage": "^1.9.0",
    "moment": "^2.29.0",
    "query-string": "^6.13.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "typeface-roboto": "^1.1.13",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "lint": "node ./node_modules/eslint/bin/eslint.js ./src/** --config .eslintrc.json --ext .ts --ext .tsx --no-inline-config --no-ignore --color --format table",
    "lint-report": "node ./node_modules/eslint/bin/eslint.js ./src/** --config .eslintrc.json --ext .ts --ext .tsx --no-inline-config --no-ignore --color --format html -o reports/eslint.html",
    "doc": "node ./node_modules/typedoc/bin/typedoc --out ./docs/documentation ./src",
    "start": "react-scripts start",
    "build": "node scripts/buildValidation.js && react-scripts build",
    "clean": "npm cache clean --force && rm -rf node_modules && rm package-lock.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "prettier": "^2.2.1",
    "typedoc": "^0.20.25"
  }
}
